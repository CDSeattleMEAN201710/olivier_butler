<!doctype HTML>
    <html>
    <head>
        <meta charset="UTF-8">
        <title>Jelly Love</title>
        <link rel="stylesheet" href="./style.css">
    </head>
    <body>
        <h1>I love me some Jellys</h1>
        <div class="jelly_container">
            <% for (jellyfish of jellys) { %>
                <div class="single_jelly">
                    <h2>Name <%= jellyfish.name %></h2>
                    <ul>
                        <li>Superpower: <%= jellyfish.superpower %></li>
                        <li>Squishability: <%= jellyfish.squishiness %></li>
                    </ul>
                </div>
            <% } %>
        </div>
    </body>
</html>